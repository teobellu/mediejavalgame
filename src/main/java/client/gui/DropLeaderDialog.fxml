<AnchorPane maxHeight="300.0" maxWidth="600.0" minHeight="300.0" minWidth="600.0" prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.131" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.gui.DropLeaderController">
   <children>
      <GridPane layoutX="35.0" layoutY="135.0" AnchorPane.leftAnchor="35.0" AnchorPane.rightAnchor="35.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="160.0" minWidth="10.0" prefWidth="132.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="201.0" minWidth="10.0" prefWidth="198.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <ChoiceBox fx:id="_choiceBox" prefHeight="26.0" prefWidth="241.0" GridPane.columnIndex="1" />
            <Label text="Select Leader card:" />
         </children>
      </GridPane>
      <ButtonBar layoutX="186.0" layoutY="146.0" prefHeight="40.0" prefWidth="200.0" AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="14.0">
        <buttons>
          <Button fx:id="_okButton" defaultButton="true" mnemonicParsing="false" onAction="#onOkClicked" text="OK" />
            <Button fx:id="_cancelButton" mnemonicParsing="false" onAction="#onCancelClicked" text="Cancel" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
